    "version": "0.2.0",
    "configurations": [
  
  
        {
            "name": "IV", //I named it IV because i can 
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/apex_sim", //path to your programs exe and exe name
            "args": ["input3.asm"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
  }